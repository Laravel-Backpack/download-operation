{
    "name": "backpack/download-operation",
    "description": "Quickly add a Download button to your Laravel/Eloquent entries. But let's face it, you'll use this as Download Invoice PDF, won't you?",
    "license": "MIT",
    "authors": [
        {
            "name": "Cristian Tabacitu",
            "email": "cristian.tabacitu@backpackforlaravel.com",
            "homepage": "https://backpackforlaravel.com"
        }
    ],
    "homepage": "https://github.com/laravel-backpack/download-operation",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "Download Operation", "Download PDF", "Download Snapshot", "Download Image", "Backpack Download Button"
    ],
    "require": {
        "backpack/crud": "^5.0|^6.0",
        "spatie/browsershot": "^3.41|^4.0|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Backpack\\DownloadOperation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backpack\\DownloadOperation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Backpack\\DownloadOperation\\AddonServiceProvider"
            ]
        }
    }
}
